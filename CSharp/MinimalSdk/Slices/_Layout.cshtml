@inherits RazorLayoutSlice<LayoutModel>

<!DOCTYPE html>
<html lang="en">
<head>
    <title>@Model.Title</title>
    <script type="module" src="https://cdn.jsdelivr.net/gh/starfederation/datastar@1.0.0-beta.6/bundles/datastar.js"></script>
    @await RenderSectionAsync("head")
</head>
<body>
  <nav>
    <ul>
      <li><a href="/" title="Home">Home</a></li>
      <li><a href="/quiz" title="Quiz">Quiz</a></li>
    </ul>
  </nav>
@await RenderBodyAsync()
<footer>
    @await RenderSectionAsync("footer")
</footer>
</body>
</html>
